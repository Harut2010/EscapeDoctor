<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.ZonesShift"
        tools:targetApi="31">
        <activity
            android:exported="false"
            tools:ignore="MissingClass"
            android:name="com.example.escapethedoctor.authentication.RegisterActivity" />
        <activity
            android:exported="false"
            tools:ignore="MissingClass"
            android:name="com.example.escapethedoctor.authentication.LoginActivity" />
        <activity
            android:exported="true"
            android:screenOrientation="landscape"
            tools:ignore="MissingClass"
            android:name="com.example.escapethedoctor.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>